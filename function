def gamble(prob, prize):
  import random
  pay = random.randint(1,100)
  #randomise number
  print(pay)
  if prob * prize > pay:
     return True
  else:
    return False 

prob= int(input("Type prob"))
prize = int(input("type prize"))
print(gamble(prob, prize))
