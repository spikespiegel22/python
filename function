 def my(a,b,c):
  answer = a ** b % c
  print(answer)

my(32,3,4)
my(32, 343,234)

myList = ["Cat", "Dog","Mouse"]
def search(myList):
  for x in myList:
    print(x)

search(["Cat","Dog","Rabbit"])

def hello(name)
  print("hello",name)

hello("Sandra")
hello("laura")
