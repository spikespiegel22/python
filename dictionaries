list = ["Manchester", "London", "Liverpool"]

d = {"DUB" : "Dublin",
"YYZ":"Toronto"}
#created dictionary in 
d.pop("DUB")
#deletes Dublin from dictionary d
print(d)

d["LHR"] = "London Heathrow"
#add to the dictionary d
print(d)
print(d["LHR"])
# prints city corresponting to LHR

for key in d:
  print(d[key])

g = {
"hello":["hola", "halo", "chesz"],

"yes" : ["si", "ja, ""oui"]
  
}

#for key in g:
 # print(key)
  #print(g[key])

for key in g:
  for i in g[key]:
    print(i)
